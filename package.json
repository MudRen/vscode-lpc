{
    "name": "lpc",
    "displayName": "LPC Language",
    "description": "LPmud C language support for Visual Studio Code",
    "author": {
        "name": "oiuv",
        "email": "i@oiuv.cn"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "publisher": "mudren",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "fluffos",
        "mudos",
        "lpmud",
        "mud",
        "mudren",
        "lpc"
    ],
    "homepage": "https://mud.ren",
    "repository": {
        "type": "git",
        "url": "https://github.com/MudRen/vscode-lpc.git"
    },
    "icon": "images/lpc-logo.png",
    "contributes": {
        "languages": [{
            "id": "lpc",
            "aliases": ["Lpc", "lpc"],
            "extensions": [".c",".h"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lpc",
            "scopeName": "source.lpc",
            "path": "./syntaxes/lpc.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "lpc",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
