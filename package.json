{
    "name": "lpc",
    "displayName": "LPC",
    "description": "LPC language support for Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lpc",
            "aliases": ["Lpc", "lpc"],
            "extensions": [".c",".h"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lpc",
            "scopeName": "source.lpc",
            "path": "./syntaxes/lpc.tmLanguage.json"
        }]
    }
}